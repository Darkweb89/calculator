#include<stdio.h>
#include<conio.h>
void main()
{
  int a,b,sum,sub,mul,div,modulo,pow;
  printf("enter the value of a & b");
  scanf("%d%d",&a&b);
  case '+'
  sum=a+b;
  printf("sum=%d",sum);
  break;
  case '-'
  sub=a-b;
  printf("sub=%d",sub);
  break;
  case '*'
  mul=a*b;
  printf("mul=%d",mul);
  break;
  case '/'
  div=a/b;
  printf("div=%d",div);
  break;
  case '%'
  modulo=a%b;
  printf("modulo=%d",modulo);
  break;
  case'**'
  pow=a**b;
  printf("pow=%d",pow);
  break;
  getch()
}
  
